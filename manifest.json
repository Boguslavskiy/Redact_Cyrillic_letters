{
  "manifest_version": 2,
  "name": "Cyrillic Redaction",
  "version": "1.0",
  "description": "Redacts Cyrillic letters on web pages.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["contentScript.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icons/icon48.png"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}